#default word color
#regexp=[\w.,\:\-_/]+
regexp=.+
colours=white
-

#numeric
# regexp=(?=\|)\s*(\d+)\s*($|(?=\|))
regexp=\|\s(\d+)\s*($|(?=\|))
colours=magenta
-

#table borders
regexp=[+\-=]+[+\-=]|[|]
colours=blue
-

#data in ( ) and ' '
# regexp=\([\w\d,']+\)
# colours=white
# -

#date
regexp=\d{4}-\d{2}-\d{2}
colours=green
-

#time
regexp=\d{1,2}:\d{1,2}:\d{1,2}
colours=green
-

#IP
regexp=(\d{1,3}\.){3}\d{1,3}(:\d{1,5})?
colours=blue
-

#schema
regexp=`\w+`
colours=red
-

#email
# regexp=[\w\.\-_]+@[\w\.\-_]+
# colours=magenta
# -

#row delimeter when using \G key
regexp=[*]+.+[*]+
count=stop
colours=blue
-

#column names when using \G key
regexp=^\w+\s*(?=\|)
colours=blue
