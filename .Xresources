! U R X V T

URxvt*scrollBar: false

! URxvt*font: xft:Terminus (TTF):antialias=false:hinting=false:autohint=false:pixelsize=14,xft:NanumSquare
! URxvt*boldFont: xft:Terminus (TTF):antialias=false:hinting=false:autohint=false:pixelsize=14,xft:NanumSquare
! URxvt*italicFont: xft:Terminus (TTF):antialias=false:hinting=false:autohint=false:pixelsize=14,xft:NanumSquare
! URxvt*boldItalicFont: xft:Terminus (TTF):antialias=false:hinting=false:autohint=false:pixelsize=14,xft:NanumSquare

URxvt*font: xft:Terminus (TTF):size=12,xft:NanumSquare
URxvt*boldFont: xft:Terminus (TTF):size=12,xft:NanumSquare
URxvt*italicFont: xft:Terminus (TTF):size=12,xft:NanumSquare
URxvt*boldItalicFont: xft:Terminus (TTF):size=12,xft:NanumSquare

!URxvt*font: xft:xos4 Terminus:size=10,xft:NanumSquare
!URxvt*boldFont: xft:xos4 Terminus:size=10,xft:NanumSquare
!URxvt*italicFont: xft:xos4 Terminus:size=10,xft:NanumSquare
!URxvt*boldItalicFont: xft:xos4 Terminus:size=10,xft:NanumSquare

!URxvt*letterSpace: 1
URxvt*underlineColor: #f8bbd0

!URxvt.transparent: true
!URxvt.tintColor: white
!URxvt.shading: 150
!URxvt.fadeColor: red

!URxvt*depth: 32
!URxvt*background: rgba:0000/0000/0200/c800

!URxvt*scrollBar_right: true
!URxvt*scrollColor:     #000000
!URxvt*geometry:        112x22

!URxvt.cursorBlink: 1
!URxvt.cursorUnderline: 1
!URxvt*cursorColor: #6e9db5
!URxvt*cursorColor2: #ed80a5

URxvt.perl-ext-common: default,clipboard,url-select,keyboard-select,resize-font
URxvt.url-select.launcher: chromium
URxvt.url-select.underline: true

URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search
URxvt.keyboard-select.clipboard: true

